<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>com.ApplicationOperation.WorkListeners.WorkListeners</listener-class>
  </listener>
  <servlet>
    <servlet-name>Application-Operation</servlet-name>
    <servlet-class>com.ApplicationOperation.ApplicationOperation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Application-Operation</servlet-name>
    <url-pattern>/work</url-pattern>
  </servlet-mapping>
</web-app>
